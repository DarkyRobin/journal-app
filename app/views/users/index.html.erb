<div class="container">
    <h1>Users List</h1>
    <table>
      <thead>
        <tr>
          <th>Username</th>
          <th>Actions</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= user.username %></td>
            
          </tr>
        <% end %>
      </tbody>
    </table>

  </div>